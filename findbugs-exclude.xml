<?xml version="1.0" encoding="UTF-8"?>
<!-- http://findbugs.sourceforge.net/manual/filter.html -->
<FindBugsFilter>
    <Match>
        <!-- Отключаем проверку nonnull полей не инициализированных из конструкторов (нужно для entity - т.к. нужен protected пустой конструктор) -->
        <Bug pattern="NP_NONNULL_FIELD_NOT_INITIALIZED_IN_CONSTRUCTOR"/>
    </Match>
</FindBugsFilter>
